{
  "name": "@glimmer/vm-babel-plugins",
  "type": "module",
  "version": "0.85.5",
  "description": "Compiles out VM assertion and deprecation utilities and debug tooling based on environment",
  "repository": "https://github.com/glimmerjs/glimmer.js",
  "author": "Tom Dale <tom@tomdale.net>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "mocha",
    "test:lint": "eslint .",
    "test:types": "tsc --noEmit -p ../tsconfig.json",
    "build": "rollup -c rollup.config.mjs",
    "test:publint": "publint"
  },
  "dependencies": {
    "babel-plugin-debug-macros": "^0.3.4"
  },
  "devDependencies": {
    "babel-plugin-tester": "^11.0.4",
    "eslint": "^8.52.0",
    "mocha": "^10.2.0",
    "rollup": "^3.21.6",
    "@glimmer-workspace/build-support": "workspace:^",
    "publint": "^0.2.5"
  },
  "main": "index.ts",
  "types": "index.ts",
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/dev/index.d.ts",
        "development": {
          "default": "./dist/dev/index.js"
        },
        "default": "./dist/prod/index.js"
      }
    }
  }
}
