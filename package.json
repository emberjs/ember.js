    "bower": "~1.3.2",
    "chalk": "^0.5.1",
    "ember-cli": "^1.13.13",
    "ember-cli-dependency-checker": "^1.0.1",
    "ember-cli-sauce": "^1.4.2",
    "ember-cli-yuidoc": "0.7.0",
    "ember-publisher": "0.0.7",
